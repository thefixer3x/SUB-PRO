{
  "name": "subscription-manager",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx expo start",
    "start": "npx expo start",
    "build:web": "NODE_ENV=production expo export --platform web",
    "prebuild:production": "node scripts/check-env.js",
    "build:production": "npm run prebuild:production && NODE_ENV=production expo export --platform web",
    "vercel-build": "npm run build:production",
    "build:mobile": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "deploy:preview": "eas build --profile preview",
    "deploy:production": "eas build --profile production",
    "submit:store": "eas submit --profile store-submission",
    "preview": "npx serve dist",
    "analyze": "npx source-map-explorer 'dist/static/js/*.js'",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "test": "jest",
    "bump:runtime": "node scripts/bump-runtime-version.js",
    "test:coverage": "jest --coverage",
    "lighthouse": "lhci autorun",
    "type-check": "tsc --noEmit",
    "check-env": "node scripts/check-env.js",
    "test-supabase": "node -r dotenv/config scripts/test-supabase-config.js",
    "test-ui": "node scripts/test-with-playwright.js",
    "debug-ui": "node scripts/detailed-debug.js",
    "analyze-build": "node scripts/analyze-build.js",
    "console-debug": "node scripts/console-debug.js",
    "test-prod": "node scripts/test-prod-build.js",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
