{
  "expo": {
    "name": "SubTrack Pro",
    "slug": "subtrack-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "subtrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "owner": "thefixer3x",
    "description": "The ultimate subscription management app with AI-powered insights, virtual cards, and smart spending analytics.",
    "primaryColor": "#3B82F6",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lanonasis.subpro",
      "buildNumber": "1",
      "requireFullScreen": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan subscription documents for easier data entry.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to allow you to select subscription documents.",
        "NSFaceIDUsageDescription": "Use Face ID to securely access your subscription data.",
        "NSLocationWhenInUseUsageDescription": "Location is used to find nearby merchants and improve spending insights.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      },
      "associatedDomains": [
        "applinks:subtrack-pro.lanonasis.com"
      ],
      "appStoreUrl": "https://apps.apple.com/app/subtrack-pro/id123456789"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lanonasis.subpro",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "com.google.android.gms.permission.AD_ID",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "subtrack-pro.lanonasis.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.lanonasis.subpro"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "indexHtml": "dist/index.html"
    },
    "plugins": [
      "expo-updates",
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "expo-document-picker",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you select subscription documents.",
          "cameraPermission": "The app accesses your camera to let you scan subscription documents."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SubTrack Pro to use your location to find nearby merchants and improve spending insights."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#3B82F6",
          "defaultChannel": "default"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7459389089200506~3035173479",
          "iosAppId": "ca-app-pub-7459389089200506~3544672881",
          "android_app_id": "ca-app-pub-7459389089200506~3035173479",
          "ios_app_id": "ca-app-pub-7459389089200506~3544672881",
          "user_tracking_usage_description": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c026aca9-e212-434f-bb68-65603b900112"
      },
      "router": {}
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c026aca9-e212-434f-bb68-65603b900112"
    },
    "runtimeVersion": "1.0.0"
  }
}