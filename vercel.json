{
  "version": 2,
  "buildCommand": "npx expo export -p web --output-dir dist",
  "outputDirectory": "dist",
  "framework": null,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/create-checkout-session",
      "destination": "https://subtrack-pro.lanonasis.com/.netlify/functions/create-checkout-session"
    },
    {
      "source": "/((?!_expo|static|favicon.ico|manifest.json|service-worker.js).*)",
      "destination": "/index.html"
    }
  ]
}