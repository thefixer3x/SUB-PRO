{
  "name": "sub-pro",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/SUB-PRO",
  "projectType": "application",
  "tags": ["type:mobile", "scope:subscriptions", "framework:expo", "status:live", "domain:sub-pro.seftechub.com", "stores:live"],
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 9995,
        "host": "0.0.0.0"
      }
    },
    "serve": {
      "executor": "@nx/expo:serve",
      "options": {
        "command": "web"
      },
      "configurations": {
        "development": {
          "command": "web --dev-client"
        },
        "production": {
          "command": "web --no-dev --minify"
        }
      }
    },
    "dev": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 9995,
        "host": "0.0.0.0"
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "options": {
        "platform": "android"
      }
    },
    "build": {
      "executor": "@nx/expo:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "platform": "all",
        "outputPath": "dist/apps/SUB-PRO"
      },
      "configurations": {
        "development": {
          "developmentClient": true
        },
        "preview": {
          "distribution": "internal"
        },
        "production": {
          "autoIncrement": true
        }
      }
    },
    "build-ios": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "ios"
      }
    },
    "build-android": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "android"
      }
    },
    "build-web": {
      "executor": "@nx/expo:export",
      "options": {
        "platform": "web",
        "outputDir": "dist/apps/SUB-PRO"
      }
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/SUB-PRO/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/SUB-PRO/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}